{
    "manifest_version": 3,
    "name": "Your Extension",
    "version": "1.0",
    
    "web_accessible_resources": [
      {
        "resources": [
          "assets/locales/en/translation.json",
          "assets/styles.css",
          "assets/locales/en/*",
          "assets/*.css"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
}
  